@hex:
  # Note: sequences of ordinary nops (90 90 90 ...) are added
  # for bundle alignemnt.
  
  # Recommended nops
  90
  66 90
  0F 1F 00
  0F 1F 40 00
  0F 1F 44 00 00
  66 0F 1F 44 00 00
  0F 1F 80 00 00 00 00
@dis:
     0:	90                   	nop
     1:	66 90                	xchg   %ax,%ax
     3:	0f 1f 00             	nopl   (%rax)
     6:	0f 1f 40 00          	nopl   0x0(%rax)
     a:	0f 1f 44 00 00       	nopl   0x0(%rax,%rax,1)
     f:	66 0f 1f 44 00 00    	nopw   0x0(%rax,%rax,1)
    15:	0f 1f 80 00 00 00 00 	nopl   0x0(%rax)
@rdfa_output:
  return code: 0

@hex:
  # Models a jump to the begining of an atomic instruction sequence.
  # jmp 0x2
  EB 00
  # and %edx, 0xe0
  83 e2 e0 \\
  # add %rdx, %r15
  4c 01 fa \\
  # jmp %rdx
  ff e2
@dis:
  0000000000000000: eb 00                                        jmp 0x2
  0000000000000002: 83 e2 e0                                     and %edx, 0xe0
  0000000000000005: 4c 01 fa                                     add %rdx, %r15
  0000000000000008: ff e2                                        jmp %rdx
@rdfa_output:
  return code: 0
